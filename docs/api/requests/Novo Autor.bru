meta {
  name: Novo Autor
  type: graphql
  seq: 2
}

post {
  url: {{host}}/{{path}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation NovoAutor($nomeAutor:String) {
    novoAutor(nome:$nomeAutor) {
      id
    }
  }
}

body:graphql:vars {
  {
    "nomeAutor": "Jose Saramago"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
